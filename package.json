{
  "name": "vue3-jstree",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:nocheck": "vite build",
    "preview": "vite preview",
    "format": "prettier --write",
    "prettier": "prettier --ignore-path .gitignore --write \"src/**/*.+(ts|tsx|vue)\"",
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "eventemitter3": "^4.0.7",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vitejs/plugin-vue": "^2.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "prettier": "^2.5.1",
    "sass": "^1.45.1",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue-tsc": "^0.29.8",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.6",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,vue}": [
      "npm run prettier",
      "npm run test",
      "git add ."
    ]
  }
}
